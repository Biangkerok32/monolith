<?xml version="1.0" encoding="GB2312"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
	<context:component-scan base-package="com.kongur.monolith.im.weixin.service" />
	 
	
	<bean id="defaultMessageProcessServiceFactory" class="com.kongur.monolith.im.weixin.service.message.DefaultMessageProcessServiceFactory">
	    <property name="messageProcessServiceMap">
	        <map>
	            <entry key="dvm" value-ref="developerValidateMessageProcessService"></entry>
	            <entry key="text" value-ref="textMessageProcessService"></entry>
	        </map>
	    </property>
	    <property name="defaultMessageProcessService" ref="discardMessageProcessService"></property>
	</bean>
	
	
</beans>